// programa com vetor numeros imppares e pares
#include <stdio.h>

int main()
{
    int vet[10]= {1,2,3,4,5,6,7,8,9,12};
    int i,impar,par;
    
    for (i=1;i<10;i++)
    {
        if (vet[i] % 2 == 1)
        {  
		   printf("impar [%d]\n",vet[i]);
           impar++;
        }
      else
        {  
		   printf ("par [%d]\n", vet[i]);
           par++;
        }
	}  

        printf ("quantidade de numeros impares: %d\n", impar);
        printf ("quantidade de pares: %d\n",par);
        
 }
