//questao 1

#include<stdio.h>

 int main(){
 
 float vetor[30];
 int i;
 
 for (i=0;i<30;i++)
 {
 vetor[i]= i+1;
 printf (" vetor[%d] %2.f\n",i,vetor[i]);
 }
}

//questao2 

#include<stdio.h>

 int main(){
 
 float vetor[20];
 int i;
 
 for (i=0;i<20;i++)
 {
 vetor[i]= 2*i;
 printf (" vetor[%d] %2.f\n",i,vetor[i]);
 }
}

